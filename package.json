{
  "name": "@abyrd9/zod-form-data",
  "version": "1.2.3",
  "description": "A Zod-based form data validation library",
  "license": "MIT",
  "keywords": [
    "zod",
    "form",
    "validation",
    "typescript",
    "react"
  ],
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "README.md"
  ],
  "devDependencies": {
    "@happy-dom/global-registrator": "^18.0.0",
    "@testing-library/dom": "^10.4.0",
    "@types/bun": "latest",
    "@types/react": "^19.1.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.25.67",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": ">=18.0.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.67"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": false
    },
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": true
    },
    "zod": {
      "optional": false
    }
  },
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "update": "npx npm-check-updates -u",
    "dev": "bun --hot index.html",
    "dev:tsx": "bun --hot browser.tsx",
    "publish:patch": "npm version patch && bun publish",
    "publish:minor": "npm version minor && bun publish",
    "publish:major": "npm version major && bun publish"
  },
  "sideEffects": false,
  "dependencies": {
    "@testing-library/react": "^16.3.0"
  }
}
