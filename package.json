{
  "name": "@abyrd9/zod-form-data",
  "version": "1.3.2",
  "description": "Type-safe Zod v4 utilities to flatten and unflatten data, parse FormData, and collect schema-aligned errors.",
  "license": "MIT",
  "keywords": [
    "zod",
    "form",
    "validation",
    "typescript",
    "forms",
    "utilities"
  ],
  "type": "module",
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts"
    }
  },
  "files": [
    "src",
    "index.ts",
    "README.md"
  ],
  "devDependencies": {
    "@happy-dom/global-registrator": "^18.0.1",
    "@types/bun": "latest",
    "vitest": "^3.2.4",
    "zod": "^4.1.11"
  },
  "peerDependencies": {
    "typescript": "^5.8.3",
    "zod": "^4.1.11"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": false
    },
    "zod": {
      "optional": false
    }
  },
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "update": "npx npm-check-updates -u",
    "publish:patch": "npm version patch && bun publish",
    "publish:minor": "npm version minor && bun publish",
    "publish:major": "npm version major && bun publish"
  },
  "sideEffects": false,
  "dependencies": {}
}
